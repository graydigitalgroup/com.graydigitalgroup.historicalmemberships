<?xml version="1.0"?>
<extension key="com.graydigitalgroup.historicalmemberships" type="module">
  <file>historicalmemberships</file>
  <name>com.graydigitalgroup.historicalmemberships</name>
  <description>This extension allows for displaying memberships in the past. It is used to pull historical membership reports.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Richard Baugh</author>
    <email>richard@graydigitalgroup.com</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/graydigitalgroup/com.graydigitalgroup.historicalmemberships</url>
    <url desc="Documentation">https://github.com/graydigitalgroup/com.graydigitalgroup.historicalmemberships/README.md</url>
    <url desc="Support">https://github.com/graydigitalgroup/com.graydigitalgroup.historicalmemberships/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2023-04-14</releaseDate>
  <version>1.0</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.60</ver>
  </compatibility>
  <comments>This extension is used to provide historical membership reporting.</comments>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Historicalmemberships</namespace>
    <format>23.02.1</format>
    <angularModule>crmHistoricalmemberships</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
  </mixins>
</extension>
